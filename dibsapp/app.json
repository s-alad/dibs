{
  "expo": {
    "name": "dibsapp",
    "slug": "dibsapp",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "dibs-app-scheme",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dibsapp.dibsapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dibsapp.dibsapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b3d9ed61-572f-40b5-a21c-6ad203f71cb5"
      }
    },
    "owner": "dibsapp",
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Dibs to use your location. This is needed to show you recent throw-away dibs & items near you!"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Dibs to access your camera. This is needed to upload photos of your throw-away dibs items!",
          "microphonePermission": "Allow Dibs to access your microphone, this is needed to record audio for your dibs items!",
          "recordAudioAndroid": true
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Allow Dibs to access your camera. This is needed to upload photos of your throw-away dibs items!"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
